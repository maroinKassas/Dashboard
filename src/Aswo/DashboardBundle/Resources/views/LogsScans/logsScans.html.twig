{% extends "content.html.twig" %}

{% block title %}
  	Logs Scans
{% endblock %}

{% block titleContent %}
  	Logs Scans
{% endblock %}

{% block content %}
	<iframe class="col-md-12" style="height:780px; border:0px solid gainsboro" src="http://fr-dashboardscanners.aswo.net"></iframe>
{% endblock %}

{% block javaScriptsContent %}
    <script type="text/javascript"> 
        $(document).ready(function() {
            var elements = document.getElementsByClassName("selected");
            if(elements != null && elements.length != 0) {
                elements[0].className = "notSelected";
            } 
            document.getElementById("logsScans").className = "selected";
        })
    </script>
{% endblock %}